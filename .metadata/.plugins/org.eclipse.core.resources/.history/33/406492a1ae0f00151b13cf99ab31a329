package modell;

import java.util.Observable;

public class BeobachtbarDrei extends Observable
{
    private int _n;

    public BeobachtbarDrei()
    {
        _n = 0;
    }

    public int getNumber()
    {
        return _n;
    }

    public void increment()
    {
        ++_n;
        setChanged();
        notifyObservers();
    }

}
